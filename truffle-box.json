{
  "ignore": [
    "README.md",
    ".gitignore"
  ],
  "commands": {
    "Compile contracts": "truffle compile",
    "Migrate contracts": "truffle migrate",
    "Test contracts": "truffle test"
  },
  "hooks": {
    "post-unpack": "cd client && npm install && cd lib && ln -s ../../build/contracts contracts"
  }
}

